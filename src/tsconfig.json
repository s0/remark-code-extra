{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "isolatedModules": false,
    "experimentalDecorators": false,
    "declaration": true,
    "noImplicitUseStrict": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
